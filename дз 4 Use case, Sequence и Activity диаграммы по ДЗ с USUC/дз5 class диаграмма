@startuml

class "Счет" as с
class "Карта" as к
class "Перевод" as пд
class "Отчет" as о
class "Банкомат" as б

together {
class "Пользователь" as п
class "Человек" as ч
class "Человек\n получающий\n перевод" as чпп
}

ч <|-- п
с <-- п
п <-- пд
п <-- к
ч <|-- чпп
с <|-- к
пд <.. с
чпп <-- пд
пд *-- о
б <-- к
чпп <-- с

@enduml
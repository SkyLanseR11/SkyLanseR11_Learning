@startuml

skinparam classAttributeIconSize 0

class Карта {
-Пин-код : int
-НомерКарты : int
+ВладелецКарты : char
+SetPIN()
+GetBalance()
+BlockCard() 
}

class Счет {
-Account : int
+Open()
+Close()
}

class Пользователь {
#Address : char
+MakeTransfer()
+SetAmount()
+SelectAccount()
}


class Банкомат {
-ID Банкомата : int
+СчитатьКарту() 
}

class Перевод {
}

class Отчет {
+Save()
}

class Человек {
+Name : string
-cardID : int
+PhoneNumber : char
+CreateAccount()
+BlockCard() 
}

class "Человек\n получающий\n перевод" as чпп {
+GetTransfer()
+CheckTransferInfo()
}

Банкомат <-- Карта
Карта o-- Счет
Пользователь <-- Счет
Перевод <-- Пользователь 
Человек <|-- Пользователь 
Перевод <-- Отчет
Человек <|-- чпп

@enduml
@startuml

skinparam classAttributeIconSize 0

together {

class "Перевод" as пд {
-PIN : int
-cardID : int
+SetPIN()
+GetBalance()
+BlockCard() 
}

class "Счет" as с {
-Account : int
+Open()
+Close()
}

class "Отчет" as о {
+Save()
}

}

class "Банкомат" as б {
-PIN : int
-cardID : int
+SetPIN()
+GetBalance()
+BlockCard() 
}

class "Карта" as к {
-PIN : int
-cardID : int
+SetPIN()
+GetBalance()
+BlockCard() 
}

together {

class "Пользователь" as п {
#Address : char
+MakeTransfer()
+SetAmount()
+SelectAccount()
}

class "Человек" as ч {
+Name : string
-cardID : int
+PhoneNumber : char
+CreateAccount()
+BlockCard() 
}

class "Человек\n получающий\n перевод" as чпп {
+GetTransfer()
+CheckTransferInfo()
}

}

ч <|-- п
с <-- п
ч <|-- чпп
с <|-- к
пд <.. с
чпп <-- пд
пд *-- о
б <-- к

@enduml
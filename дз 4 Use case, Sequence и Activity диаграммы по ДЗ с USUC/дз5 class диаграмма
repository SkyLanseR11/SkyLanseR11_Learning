@startuml

skinparam classAttributeIconSize 0

class Карта {
-Пин-код : int
-НомерКарты : int
+ВладелецКарты : char
+SetPIN()
+GetBalance()
+BlockCard() 
}

together {

class "Человек\n получающий\n перевод" as чпп {
+GetTransfer()
+CheckTransferInfo()
}

class Пользователь {
#Address : char
+MakeTransfer()
+SetAmount()
+SelectAccount()
}

class Человек {
+Name : string
-cardID : int
+PhoneNumber : char
+CreateAccount()
+BlockCard() 
}

class Счет {
-Account : int
+Open()
+Close()
}

}

class Перевод {
+СоздатьОтчет()
+ПроверкаСредств()
}

class Банкомат {
-ID Банкомата : int
+СчитатьКарту() 
}

class Отчет {
+Save()
}

Банкомат <-- Карта
Карта *-- Счет
Пользователь <-- Счет
Перевод <-- Пользователь 
Человек <|-- Пользователь 
Перевод *-- Отчет
Человек <|-- чпп
чпп <-- Перевод 
Пользователь <-- Отчет
Счет <-- Перевод 
Пользователь <-- Карта

@enduml